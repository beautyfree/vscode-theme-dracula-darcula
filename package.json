{
  "version": "1.0.4",
  "name": "dracula-darcula",
  "displayName": "Dracula Darcula",
  "description": "Theme based on Dracula for code editor and Darcula for workspace",
  "publisher": "devall",
  "license": "MIT",
  "scripts": {
    "build": "node ./scripts/build.js",
    "lint": "node ./scripts/lint.js",
    "package": "vsce package -o ./bin/dracula-darcula.vsix",
    "vscode:prepublish": "npm run build",
    "vsce-publish": "vsce publish"
  },
  "maintainers": [
    "Alexey Elizarov <felex-ae@ya.ru>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/beautyfree/vscode-theme-dracula-darcula.git"
  },
  "bugs": {
    "url": "https://github.com/beautyfree/vscode-theme-dracula-darcula/issues"
  },
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "dracula",
    "darcula",
    "dark",
    "theme",
    "color-theme"
  ],
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#3B3E40",
    "theme": "dark"
  },
  "contributes": {
    "themes": [
      {
        "label": "Dracula Darcula",
        "uiTheme": "vs-dark",
        "path": "./theme/dracula-darcula.json"
      }
    ]
  },
  "devDependencies": {
    "js-yaml": "3.14.1",
    "prettier": "2.2.1",
    "vsce": "1.87.0"
  }
}
